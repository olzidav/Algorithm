class Solution(object):
    def rob(self, nums):
     
        if not nums:
            return 0
        
        n = len(nums)
        
       
        if n == 1:
            return nums[0]
        
      
        if n == 2:
            return max(nums[0], nums[1])
        
       
        dp = [0] * n
        
      
        dp[0] = nums[0]
        dp[1] = max(nums[0], nums[1])
        
       
        for i in range(2, n):
            dp[i] = max(dp[i - 1], dp[i - 2] + nums[i])
        
       
        return dp[-1]


if __name__ == "__main__":
    solution = Solution()
    
    nums1 = [1, 2, 3, 1]
    print(solution.rob(nums1))  
    

    nums2 = [2, 7, 9, 3, 1]
    print(solution.rob(nums2))  
